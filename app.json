{
  "expo": {
    "name": "Rize Spring",
    "slug": "rizespring",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router"
    ],
    "orientation": "portrait",
    "icon": "./assets/appImage.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/appImage.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": false,
        "UIStatusBarHidden": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appImage.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.optisoft.rizespring",
      "statusBar": {
        "hidden": false,
        "backgroundColor": "#ffffff",
        "translucent": false,
        "barStyle": "dark-content"
      },
      "navigationBar": {
        "visible": "sticky-immersive",
        "backgroundColor": "#ffffff",
        "barStyle": "dark-content"
      }
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9529bf79-128d-467b-969d-39ee6c1129e3"
      }
    }
  }
}